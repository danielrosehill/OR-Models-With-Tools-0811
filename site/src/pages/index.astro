---
const title = "Agentic LLM Price Comparisons";
const description = "Cost & Context Analysis of 218+ LLM Models with Tool-Calling Capabilities from OpenRouter";
const lastUpdated = "November 8, 2025";
const baseUrl = import.meta.env.BASE_URL;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content={description} />
    <meta name="base-url" content={baseUrl} />
    <title>{title}</title>
    <style>
      :root {
        --bg-primary: #0f172a;
        --bg-secondary: #1e293b;
        --bg-card: #334155;
        --text-primary: #f1f5f9;
        --text-secondary: #cbd5e1;
        --text-muted: #94a3b8;
        --accent: #3b82f6;
        --accent-hover: #2563eb;
        --border: #475569;
        --success: #10b981;
        --warning: #f59e0b;
        --danger: #ef4444;
        --quadrant-1: #10b981;
        --quadrant-2: #3b82f6;
        --quadrant-3: #f59e0b;
        --quadrant-4: #ef4444;
      }

      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
      }

      body {
        font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, sans-serif;
        background: var(--bg-primary);
        color: var(--text-primary);
        line-height: 1.6;
        padding: 2rem 1rem;
      }

      .container {
        max-width: 1400px;
        margin: 0 auto;
      }

      header {
        text-align: center;
        margin-bottom: 3rem;
        padding-bottom: 2rem;
        border-bottom: 2px solid var(--border);
      }

      h1 {
        font-size: 2.5rem;
        margin-bottom: 0.5rem;
        background: linear-gradient(135deg, var(--accent), var(--success));
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        background-clip: text;
      }

      .subtitle {
        color: var(--text-secondary);
        font-size: 1.1rem;
        margin-bottom: 0.5rem;
      }

      .timestamp {
        color: var(--text-muted);
        font-size: 0.9rem;
      }

      .intro {
        background: var(--bg-secondary);
        padding: 1.5rem;
        border-radius: 0.5rem;
        margin-bottom: 2rem;
        border-left: 4px solid var(--accent);
      }

      .intro h2 {
        font-size: 1.3rem;
        margin-bottom: 0.75rem;
        color: var(--text-primary);
      }

      .intro p {
        color: var(--text-secondary);
        margin-bottom: 0.5rem;
      }

      .stats-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
        gap: 1.5rem;
        margin-bottom: 2rem;
      }

      .stat-card {
        background: var(--bg-secondary);
        padding: 1.5rem;
        border-radius: 0.5rem;
        border: 1px solid var(--border);
        transition: transform 0.2s, box-shadow 0.2s;
      }

      .stat-card:hover {
        transform: translateY(-2px);
        box-shadow: 0 4px 12px rgba(59, 130, 246, 0.2);
      }

      .stat-label {
        color: var(--text-muted);
        font-size: 0.9rem;
        text-transform: uppercase;
        letter-spacing: 0.05em;
        margin-bottom: 0.5rem;
      }

      .stat-value {
        font-size: 2rem;
        font-weight: bold;
        color: var(--accent);
      }

      .controls {
        display: flex;
        flex-wrap: wrap;
        gap: 1rem;
        margin-bottom: 2rem;
        align-items: center;
      }

      .search-box {
        flex: 1;
        min-width: 250px;
      }

      .search-box input {
        width: 100%;
        padding: 0.75rem 1rem;
        background: var(--bg-secondary);
        border: 1px solid var(--border);
        border-radius: 0.375rem;
        color: var(--text-primary);
        font-size: 1rem;
      }

      .search-box input:focus {
        outline: none;
        border-color: var(--accent);
        box-shadow: 0 0 0 3px rgba(59, 130, 246, 0.1);
      }

      .filter-group {
        display: flex;
        gap: 0.5rem;
        flex-wrap: wrap;
      }

      .filter-btn {
        padding: 0.5rem 1rem;
        background: var(--bg-secondary);
        border: 1px solid var(--border);
        border-radius: 0.375rem;
        color: var(--text-secondary);
        cursor: pointer;
        transition: all 0.2s;
        font-size: 0.9rem;
      }

      .filter-btn:hover {
        background: var(--bg-card);
        border-color: var(--accent);
      }

      .filter-btn.active {
        background: var(--accent);
        border-color: var(--accent);
        color: white;
      }

      .chart-container {
        background: var(--bg-secondary);
        padding: 2rem;
        border-radius: 0.5rem;
        margin-bottom: 2rem;
        border: 1px solid var(--border);
      }

      .chart-container h2 {
        margin-bottom: 1.5rem;
        color: var(--text-primary);
      }

      #scatterChart {
        max-height: 500px;
      }

      .table-container {
        background: var(--bg-secondary);
        border-radius: 0.5rem;
        overflow: hidden;
        border: 1px solid var(--border);
      }

      .table-header {
        padding: 1.5rem;
        border-bottom: 1px solid var(--border);
      }

      .table-header h2 {
        color: var(--text-primary);
      }

      .table-wrapper {
        overflow-x: auto;
      }

      table {
        width: 100%;
        border-collapse: collapse;
      }

      thead {
        background: var(--bg-card);
        position: sticky;
        top: 0;
        z-index: 10;
      }

      th {
        padding: 1rem;
        text-align: left;
        font-weight: 600;
        color: var(--text-primary);
        border-bottom: 2px solid var(--border);
        cursor: pointer;
        user-select: none;
        white-space: nowrap;
      }

      th:hover {
        background: var(--bg-primary);
      }

      th.sortable::after {
        content: ' ⇅';
        opacity: 0.3;
      }

      th.sorted-asc::after {
        content: ' ↑';
        opacity: 1;
      }

      th.sorted-desc::after {
        content: ' ↓';
        opacity: 1;
      }

      td {
        padding: 1rem;
        border-bottom: 1px solid var(--border);
        color: var(--text-secondary);
      }

      tbody tr {
        transition: background-color 0.2s;
      }

      tbody tr:hover {
        background: var(--bg-card);
      }

      tbody tr.vendor-group-even {
        background: rgba(30, 41, 59, 0.3);
      }

      tbody tr.vendor-group-odd {
        background: rgba(15, 23, 42, 0.3);
      }

      tbody tr.vendor-group-even:hover {
        background: rgba(51, 65, 85, 0.5);
      }

      tbody tr.vendor-group-odd:hover {
        background: rgba(30, 41, 59, 0.5);
      }

      .vendor-divider {
        height: 2px;
        background: var(--border);
      }

      .vendor-divider td {
        padding: 0;
        height: 2px;
      }

      .quadrant-badge {
        display: inline-block;
        padding: 0.25rem 0.75rem;
        border-radius: 1rem;
        font-size: 0.85rem;
        font-weight: 600;
        text-transform: uppercase;
      }

      .quadrant-1 { background: var(--quadrant-1); color: white; }
      .quadrant-2 { background: var(--quadrant-2); color: white; }
      .quadrant-3 { background: var(--quadrant-3); color: white; }
      .quadrant-4 { background: var(--quadrant-4); color: white; }

      .vendor-tag {
        display: inline-block;
        padding: 0.25rem 0.5rem;
        background: var(--bg-card);
        border-radius: 0.25rem;
        font-size: 0.85rem;
        color: var(--text-muted);
      }

      .price-cell {
        font-family: 'Monaco', 'Courier New', monospace;
        font-size: 0.9rem;
      }

      .free-model {
        color: var(--success);
        font-weight: 600;
      }

      footer {
        margin-top: 3rem;
        padding-top: 2rem;
        border-top: 1px solid var(--border);
        text-align: center;
        color: var(--text-muted);
        font-size: 0.9rem;
      }

      footer a {
        color: var(--accent);
        text-decoration: none;
      }

      footer a:hover {
        text-decoration: underline;
      }

      @media (max-width: 768px) {
        h1 {
          font-size: 1.75rem;
        }

        .stats-grid {
          grid-template-columns: 1fr;
        }

        .controls {
          flex-direction: column;
          align-items: stretch;
        }

        .search-box {
          width: 100%;
        }

        table {
          font-size: 0.85rem;
        }

        th, td {
          padding: 0.5rem;
        }
      }

      .loading {
        text-align: center;
        padding: 3rem;
        color: var(--text-muted);
      }

      .error {
        background: var(--danger);
        color: white;
        padding: 1rem;
        border-radius: 0.5rem;
        margin: 1rem 0;
      }
    </style>
  </head>
  <body>
    <div class="container">
      <header>
        <h1>{title}</h1>
        <p class="subtitle">{description}</p>
        <p class="timestamp">Data Snapshot: {lastUpdated}</p>
      </header>

      <div class="intro">
        <h2>About This Analysis</h2>
        <p>
          This analysis examines <strong>218+ LLM models with tool-calling capabilities</strong>
          available through OpenRouter, focusing on two key dimensions:
        </p>
        <ul style="margin: 1rem 0 0 1.5rem; color: var(--text-secondary);">
          <li><strong>Cost per token</strong> (input and output pricing)</li>
          <li><strong>Context window size</strong> (maximum tokens supported)</li>
        </ul>
        <p style="margin-top: 1rem;">
          Models are categorized into quadrants using median values as dividing lines,
          allowing you to explore options across different cost and context combinations.
        </p>
      </div>

      <div class="stats-grid" id="statsGrid">
        <div class="stat-card">
          <div class="stat-label">Total Models</div>
          <div class="stat-value">--</div>
        </div>
        <div class="stat-card">
          <div class="stat-label">Price Range</div>
          <div class="stat-value">--</div>
        </div>
        <div class="stat-card">
          <div class="stat-label">Context Range</div>
          <div class="stat-value">--</div>
        </div>
        <div class="stat-card">
          <div class="stat-label">Vendors</div>
          <div class="stat-value">--</div>
        </div>
      </div>

      <div class="chart-container">
        <h2>Model Distribution by Cost & Context</h2>
        <p style="color: var(--text-muted); margin-bottom: 1rem; font-size: 0.95rem;">
          Models are stratified by cost (output price per million tokens) and context window size,
          divided at median values to create four quadrants for easier comparison.
        </p>
        <canvas id="scatterChart"></canvas>
      </div>

      <div class="chart-container">
        <h2>Models by Vendor</h2>
        <p style="color: var(--text-muted); margin-bottom: 1rem; font-size: 0.95rem;">
          Total count of tool-capable models available from each vendor.
        </p>
        <canvas id="vendorChart"></canvas>
      </div>

      <div class="controls">
        <div class="search-box">
          <input
            type="text"
            id="searchInput"
            placeholder="Search by model name or vendor..."
          />
        </div>
        <div class="filter-group">
          <button class="filter-btn active" data-filter="all">All Models</button>
          <button class="filter-btn" data-filter="free">Free Only</button>
          <button class="filter-btn" data-filter="quadrant-1">Q1: Low Cost, High Context</button>
          <button class="filter-btn" data-filter="quadrant-2">Q2: High Cost, High Context</button>
          <button class="filter-btn" data-filter="quadrant-3">Q3: Low Cost, Low Context</button>
          <button class="filter-btn" data-filter="quadrant-4">Q4: High Cost, Low Context</button>
        </div>
      </div>

      <div class="table-container">
        <div class="table-header">
          <h2>Model Directory</h2>
        </div>
        <div class="table-wrapper">
          <table id="modelsTable">
            <thead>
              <tr>
                <th class="sortable" data-sort="model_name">Model Name</th>
                <th class="sortable" data-sort="vendor">Vendor</th>
                <th class="sortable" data-sort="context_length">Context Length</th>
                <th class="sortable" data-sort="input_price_usd_per_m">Input Price ($/M)</th>
                <th class="sortable" data-sort="output_price_usd_per_m">Output Price ($/M)</th>
                <th class="sortable" data-sort="quadrant">Quadrant</th>
              </tr>
            </thead>
            <tbody id="tableBody">
              <tr>
                <td colspan="6" class="loading">Loading models...</td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>

      <footer>
        <p>
          Data sourced from <a href="https://openrouter.ai" target="_blank" rel="noopener">OpenRouter</a> |
          View on <a href="https://huggingface.co/spaces/danielrosehill/Agentic-LLM-Price-Comparisons" target="_blank" rel="noopener">Hugging Face</a>
        </p>
        <p style="margin-top: 0.5rem;">
          Created by <a href="https://danielrosehill.com" target="_blank" rel="noopener">Daniel Rosehill</a>
        </p>
      </footer>
    </div>

    <script>
      import Papa from 'papaparse';
      import { Chart, registerables } from 'chart.js';
      import annotationPlugin from 'chartjs-plugin-annotation';
      Chart.register(...registerables, annotationPlugin);

      let allModels = [];
      let filteredModels = [];
      let currentSort = { column: null, direction: 'asc' };
      let chart = null;
      let vendorChart = null;

      // Vendor name mapping
      const vendorNameMap = {
        'meta-llama': 'Meta',
        'openai': 'OpenAI',
        'qwen': 'Qwen',
        'anthropic': 'Anthropic',
        'google': 'Google',
        'mistralai': 'Mistral AI',
        'cohere': 'Cohere',
        'x-ai': 'xAI',
        'deepseek': 'DeepSeek',
        'alibaba': 'Alibaba',
        'microsoft': 'Microsoft',
        'perplexity': 'Perplexity',
        'inflection': 'Inflection',
        'ai21': 'AI21 Labs'
      };

      function normalizeVendorName(vendor) {
        return vendorNameMap[vendor.toLowerCase()] || vendor;
      }

      function cleanModelName(modelName, vendor) {
        // Remove vendor prefix from model name if it's duplicated
        const vendorPrefixes = [
          vendor,
          vendor.replace('-', ''),
          vendor.replace(' ', ''),
          vendorNameMap[vendor.toLowerCase()]
        ].filter(Boolean);

        let cleaned = modelName;
        for (const prefix of vendorPrefixes) {
          const regex = new RegExp(`^${prefix}[\\s-/:]`, 'i');
          if (regex.test(cleaned)) {
            cleaned = cleaned.replace(regex, '');
            break;
          }
        }
        return cleaned;
      }

      // Load and parse CSV data
      async function loadData() {
        try {
          const baseUrl = document.querySelector('meta[name="base-url"]')?.content || '';
          const response = await fetch(`${baseUrl}/quadrants.csv`);
          const csvText = await response.text();

          const result = Papa.parse(csvText, {
            header: true,
            dynamicTyping: true,
            skipEmptyLines: true
          });

          allModels = result.data.map(row => ({
            ...row,
            vendor: normalizeVendorName(row.vendor),
            model_name: cleanModelName(row.model_name, row.vendor),
            quadrant: determineQuadrant(row)
          }));

          filteredModels = [...allModels];

          updateStats();
          renderTable();
          createChart();
          createVendorChart();
        } catch (error) {
          console.error('Error loading data:', error);
          document.getElementById('tableBody').innerHTML =
            '<tr><td colspan="6" class="error">Error loading data. Please refresh the page.</td></tr>';
        }
      }

      function determineQuadrant(model) {
        // Calculate median values
        const prices = allModels.map(m => m.output_price_usd_per_m).filter(p => p > 0);
        const contexts = allModels.map(m => m.context_length).filter(c => c > 0);

        const medianPrice = median(prices) || 2.5;
        const medianContext = median(contexts) || 100000;

        const isFree = model.output_price_usd_per_m === 0;
        const isLowCost = model.output_price_usd_per_m <= medianPrice;
        const isHighContext = model.context_length >= medianContext;

        if (isFree) return 'Free';
        if (isLowCost && isHighContext) return 'Q1';
        if (!isLowCost && isHighContext) return 'Q2';
        if (isLowCost && !isHighContext) return 'Q3';
        return 'Q4';
      }

      function median(arr) {
        if (arr.length === 0) return 0;
        const sorted = [...arr].sort((a, b) => a - b);
        const mid = Math.floor(sorted.length / 2);
        return sorted.length % 2 ? sorted[mid] : (sorted[mid - 1] + sorted[mid]) / 2;
      }

      function updateStats() {
        const totalModels = allModels.length;
        const prices = allModels.map(m => m.output_price_usd_per_m).filter(p => p > 0);
        const contexts = allModels.map(m => m.context_length);
        const vendors = new Set(allModels.map(m => m.vendor));

        const minPrice = Math.min(...prices);
        const maxPrice = Math.max(...prices);
        const minContext = Math.min(...contexts);
        const maxContext = Math.max(...contexts);

        const stats = [
          { label: 'Total Models', value: totalModels },
          { label: 'Price Range', value: `$${minPrice.toFixed(2)} - $${maxPrice.toFixed(2)}` },
          { label: 'Context Range', value: `${formatNumber(minContext)} - ${formatNumber(maxContext)}` },
          { label: 'Vendors', value: vendors.size }
        ];

        const statsGrid = document.getElementById('statsGrid');
        statsGrid.innerHTML = stats.map(stat => `
          <div class="stat-card">
            <div class="stat-label">${stat.label}</div>
            <div class="stat-value">${stat.value}</div>
          </div>
        `).join('');
      }

      function formatNumber(num) {
        if (num >= 1000000) return (num / 1000000).toFixed(1) + 'M';
        if (num >= 1000) return (num / 1000).toFixed(0) + 'K';
        return num.toString();
      }

      function renderTable() {
        const tbody = document.getElementById('tableBody');

        if (filteredModels.length === 0) {
          tbody.innerHTML = '<tr><td colspan="6" style="text-align: center; padding: 2rem;">No models match your filters</td></tr>';
          return;
        }

        // Sort by vendor first, then by model name
        const sortedModels = [...filteredModels].sort((a, b) => {
          if (a.vendor !== b.vendor) {
            return a.vendor.localeCompare(b.vendor);
          }
          return a.model_name.localeCompare(b.model_name);
        });

        let currentVendor = '';
        let vendorIndex = 0;
        const rows = sortedModels.map((model, index) => {
          const isNewVendor = model.vendor !== currentVendor;
          if (isNewVendor) {
            currentVendor = model.vendor;
            vendorIndex++;
          }

          const vendorClass = vendorIndex % 2 === 0 ? 'vendor-group-even' : 'vendor-group-odd';
          const vendorDivider = isNewVendor && index > 0
            ? '<tr class="vendor-divider"><td colspan="6"></td></tr>'
            : '';

          return `${vendorDivider}
          <tr class="${vendorClass}">
            <td><strong>${model.model_name}</strong></td>
            <td><span class="vendor-tag">${model.vendor}</span></td>
            <td>${formatNumber(model.context_length)}</td>
            <td class="price-cell ${model.input_price_usd_per_m === 0 ? 'free-model' : ''}">
              ${model.input_price_usd_per_m === 0 ? 'FREE' : '$' + model.input_price_usd_per_m.toFixed(2)}
            </td>
            <td class="price-cell ${model.output_price_usd_per_m === 0 ? 'free-model' : ''}">
              ${model.output_price_usd_per_m === 0 ? 'FREE' : '$' + model.output_price_usd_per_m.toFixed(2)}
            </td>
            <td>
              <span class="quadrant-badge quadrant-${model.quadrant.toLowerCase().replace(' ', '-')}">
                ${model.quadrant}
              </span>
            </td>
          </tr>`;
        }).join('');

        tbody.innerHTML = rows;
      }

      function createChart() {
        const ctx = document.getElementById('scatterChart');

        // Calculate median values for quadrant lines
        const prices = allModels.map(m => m.output_price_usd_per_m).filter(p => p > 0);
        const contexts = allModels.map(m => m.context_length).filter(c => c > 0);
        const medianPrice = median(prices) || 2.5;
        const medianContext = median(contexts) || 100000;

        // Group by quadrant
        const datasets = ['Q1', 'Q2', 'Q3', 'Q4', 'Free'].map(quadrant => {
          const models = allModels.filter(m => m.quadrant === quadrant && m.output_price_usd_per_m > 0);

          const colors = {
            'Q1': 'rgba(16, 185, 129, 0.6)',
            'Q2': 'rgba(59, 130, 246, 0.6)',
            'Q3': 'rgba(245, 158, 11, 0.6)',
            'Q4': 'rgba(239, 68, 68, 0.6)',
            'Free': 'rgba(168, 85, 247, 0.6)'
          };

          return {
            label: quadrant === 'Q1' ? 'Low Cost, High Context' :
                   quadrant === 'Q2' ? 'High Cost, High Context' :
                   quadrant === 'Q3' ? 'Low Cost, Low Context' :
                   quadrant === 'Q4' ? 'High Cost, Low Context' : 'Free Models',
            data: models.map(m => ({
              x: m.context_length,
              y: m.output_price_usd_per_m,
              label: m.model_name
            })),
            backgroundColor: colors[quadrant],
            borderColor: colors[quadrant].replace('0.6', '1'),
            borderWidth: 1
          };
        });

        chart = new Chart(ctx, {
          type: 'scatter',
          data: { datasets },
          options: {
            responsive: true,
            maintainAspectRatio: true,
            plugins: {
              legend: {
                position: 'top',
                labels: { color: '#f1f5f9' }
              },
              tooltip: {
                callbacks: {
                  label: (context) => {
                    const point = context.raw;
                    return [
                      point.label,
                      `Context: ${formatNumber(point.x)} tokens`,
                      `Output: $${point.y.toFixed(2)}/M`
                    ];
                  }
                }
              },
              annotation: {
                annotations: {
                  verticalLine: {
                    type: 'line',
                    xMin: medianContext,
                    xMax: medianContext,
                    borderColor: 'rgba(241, 245, 249, 0.5)',
                    borderWidth: 2,
                    borderDash: [5, 5],
                    label: {
                      display: true,
                      content: `Median Context: ${formatNumber(medianContext)}`,
                      position: 'start',
                      color: '#f1f5f9',
                      backgroundColor: 'rgba(15, 23, 42, 0.8)',
                      padding: 6
                    }
                  },
                  horizontalLine: {
                    type: 'line',
                    yMin: medianPrice,
                    yMax: medianPrice,
                    borderColor: 'rgba(241, 245, 249, 0.5)',
                    borderWidth: 2,
                    borderDash: [5, 5],
                    label: {
                      display: true,
                      content: `Median Price: $${medianPrice.toFixed(2)}`,
                      position: 'end',
                      color: '#f1f5f9',
                      backgroundColor: 'rgba(15, 23, 42, 0.8)',
                      padding: 6
                    }
                  }
                }
              }
            },
            scales: {
              x: {
                type: 'logarithmic',
                title: {
                  display: true,
                  text: 'Context Length (tokens)',
                  color: '#f1f5f9'
                },
                ticks: {
                  color: '#cbd5e1',
                  callback: (value) => formatNumber(value)
                },
                grid: { color: '#475569' }
              },
              y: {
                type: 'logarithmic',
                title: {
                  display: true,
                  text: 'Output Price (USD per million tokens)',
                  color: '#f1f5f9'
                },
                ticks: {
                  color: '#cbd5e1',
                  callback: (value) => '$' + value.toFixed(2)
                },
                grid: { color: '#475569' }
              }
            }
          }
        });
      }

      function createVendorChart() {
        const ctx = document.getElementById('vendorChart');

        // Count models by vendor
        const vendorCounts = {};
        allModels.forEach(model => {
          vendorCounts[model.vendor] = (vendorCounts[model.vendor] || 0) + 1;
        });

        // Sort vendors by count (descending)
        const sortedVendors = Object.entries(vendorCounts)
          .sort((a, b) => b[1] - a[1]);

        const labels = sortedVendors.map(v => v[0]);
        const data = sortedVendors.map(v => v[1]);

        vendorChart = new Chart(ctx, {
          type: 'bar',
          data: {
            labels: labels,
            datasets: [{
              label: 'Number of Models',
              data: data,
              backgroundColor: 'rgba(59, 130, 246, 0.6)',
              borderColor: 'rgba(59, 130, 246, 1)',
              borderWidth: 1
            }]
          },
          options: {
            responsive: true,
            maintainAspectRatio: true,
            indexAxis: 'y', // Horizontal bar chart
            plugins: {
              legend: {
                display: false
              },
              tooltip: {
                callbacks: {
                  label: (context) => {
                    return `${context.parsed.x} model${context.parsed.x !== 1 ? 's' : ''}`;
                  }
                }
              }
            },
            scales: {
              x: {
                beginAtZero: true,
                ticks: {
                  color: '#cbd5e1',
                  stepSize: 1
                },
                grid: { color: '#475569' },
                title: {
                  display: true,
                  text: 'Number of Models',
                  color: '#f1f5f9'
                }
              },
              y: {
                ticks: {
                  color: '#cbd5e1'
                },
                grid: { display: false }
              }
            }
          }
        });
      }

      // Event listeners
      document.addEventListener('DOMContentLoaded', () => {
        loadData();

        // Search functionality
        const searchInput = document.getElementById('searchInput');
        searchInput.addEventListener('input', (e) => {
          const query = e.target.value.toLowerCase();
          filteredModels = allModels.filter(model =>
            model.model_name.toLowerCase().includes(query) ||
            model.vendor.toLowerCase().includes(query)
          );
          renderTable();
        });

        // Filter buttons
        document.querySelectorAll('.filter-btn').forEach(btn => {
          btn.addEventListener('click', (e) => {
            document.querySelectorAll('.filter-btn').forEach(b => b.classList.remove('active'));
            e.target.classList.add('active');

            const filter = e.target.dataset.filter;

            if (filter === 'all') {
              filteredModels = [...allModels];
            } else if (filter === 'free') {
              filteredModels = allModels.filter(m => m.output_price_usd_per_m === 0);
            } else if (filter.startsWith('quadrant-')) {
              const quadrant = 'Q' + filter.split('-')[1]; // Extract Q1, Q2, Q3, Q4
              filteredModels = allModels.filter(m => m.quadrant === quadrant);
            }

            renderTable();
          });
        });

        // Sorting
        document.querySelectorAll('th.sortable').forEach(th => {
          th.addEventListener('click', () => {
            const column = th.dataset.sort;

            if (currentSort.column === column) {
              currentSort.direction = currentSort.direction === 'asc' ? 'desc' : 'asc';
            } else {
              currentSort.column = column;
              currentSort.direction = 'asc';
            }

            // Update header classes
            document.querySelectorAll('th').forEach(h => {
              h.classList.remove('sorted-asc', 'sorted-desc');
            });
            th.classList.add(`sorted-${currentSort.direction}`);

            // Sort data
            filteredModels.sort((a, b) => {
              let aVal = a[column];
              let bVal = b[column];

              if (typeof aVal === 'string') {
                aVal = aVal.toLowerCase();
                bVal = bVal.toLowerCase();
              }

              if (currentSort.direction === 'asc') {
                return aVal > bVal ? 1 : -1;
              } else {
                return aVal < bVal ? 1 : -1;
              }
            });

            renderTable();
          });
        });
      });
    </script>
  </body>
</html>
